    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^2.11.5",
    "webpack-node-externals": "^1.7.2",
    "webpack-split-chunks": "^0.2.1"
  },
  "dependencies": {
    "css-loader": "^0.27.1",
    "purifycss": "^1.1.9",
    "style-loader": "^0.13.2"
