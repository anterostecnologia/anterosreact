    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.6",
