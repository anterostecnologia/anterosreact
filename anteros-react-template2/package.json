{
    "name": "@anterostecnologia/anteros-react-template2",
    "version": "1.0.21",
    "description": "Anteros components for React - Template2 Typescript",
    "main": "dist/@anterostecnologia/anteros-react-template2",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "source": "src/index.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build:umd": "cross-env BUILD_MODE=umd webpack",
        "build:umd-min": "cross-env BUILD_MODE=umd-min webpack",
        "build:types": "tsc --outDir ./dist --declaration --emitDeclarationOnly",
        "build": "npm run build:umd && npm run build:umd-min && npm run build:types",
        "build:watch": "npm run build -- --watch",
        "clean": "rimraf dist",
        "tag": "git tag -a \"v$npm_package_version\" -m \"tag version $npm_package_version\" && git push origin master --tags",
        "lint": "eslint --ext=ts,tsx src test --fix",
        "test": "npx mocha -R spec --require ts-node/register ./test/setup.js ./test/setup.js test/*_test.{ts,tsx}",
        "docs": "typedoc src/index.ts && gh-pages -d docs"
    },
    "author": "Edson Martins",
    "license": "Apache 2.0",
    "devDependencies": {
        "@anterostecnologia/anteros-react-admin": "^2.0.40",
        "@anterostecnologia/anteros-react-api2": "^1.0.7",
        "@anterostecnologia/anteros-react-buttons": "^2.0.23",
        "@anterostecnologia/anteros-react-containers": "^2.0.30",
        "@anterostecnologia/anteros-react-core": "^2.0.29",
        "@anterostecnologia/anteros-react-datasource": "^2.0.29",
        "@anterostecnologia/anteros-react-label": "^2.0.12",
        "@anterostecnologia/anteros-react-layout": "^2.0.10",
        "@anterostecnologia/anteros-react-loaders": "^2.0.8",
        "@anterostecnologia/anteros-react-masonry": "^2.0.9",
        "@anterostecnologia/anteros-react-navigation": "^2.0.9",
        "@anterostecnologia/anteros-react-notification": "^2.0.10",
        "@anterostecnologia/anteros-react-querybuilder": "^2.0.49",
        "@anterostecnologia/anteros-react-table": "^2.0.18",
        "@types/node": "^17.0.21",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-redux": "^7.1.24",
        "@types/react-router-dom": "^5.3.3",
        "axios": "^0.27.2",
        "cross-env": "^7.0.3",
        "qs": "^6.11.0",
        "react": "^16.14.0",
        "react-addons-shallow-compare": "^15.6.3",
        "react-dom": "^16.14.0",
        "react-loader-spinner": "^5.1.7-beta.1",
        "react-redux": "^8.0.2",
        "react-router": "^5.3.3",
        "react-router-dom": "^5.3.3",
        "react-router-tabs": "^1.3.2",
        "regenerator-runtime": "^0.13.9",
        "rimraf": "^3.0.2",
        "ts-loader": "^9.4.1",
        "typedoc": "^0.23.15",
        "typescript": "^4.7.4",
        "universal-base64": "^2.1.0",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-node-externals": "^3.0.0"
    },
    "peerDependencies": {
        "react": "^16.14.0",
        "react-dom": "^16.14.0"
    }
}
